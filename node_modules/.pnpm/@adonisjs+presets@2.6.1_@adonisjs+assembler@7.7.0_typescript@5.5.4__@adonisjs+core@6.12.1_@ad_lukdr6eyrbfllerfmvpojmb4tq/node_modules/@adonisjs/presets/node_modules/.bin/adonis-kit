#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/vmuser/RATP/RER/5_AUTRES/starter_web_app/node_modules/.pnpm/@adonisjs+core@6.12.1_@adonisjs+assembler@7.7.0_typescript@5.5.4__@vinejs+vine@2.1.0_edge.js@6.0.2/node_modules/@adonisjs/core/build/toolkit/node_modules:/home/vmuser/RATP/RER/5_AUTRES/starter_web_app/node_modules/.pnpm/@adonisjs+core@6.12.1_@adonisjs+assembler@7.7.0_typescript@5.5.4__@vinejs+vine@2.1.0_edge.js@6.0.2/node_modules/@adonisjs/core/build/node_modules:/home/vmuser/RATP/RER/5_AUTRES/starter_web_app/node_modules/.pnpm/@adonisjs+core@6.12.1_@adonisjs+assembler@7.7.0_typescript@5.5.4__@vinejs+vine@2.1.0_edge.js@6.0.2/node_modules/@adonisjs/core/node_modules:/home/vmuser/RATP/RER/5_AUTRES/starter_web_app/node_modules/.pnpm/@adonisjs+core@6.12.1_@adonisjs+assembler@7.7.0_typescript@5.5.4__@vinejs+vine@2.1.0_edge.js@6.0.2/node_modules/@adonisjs/node_modules:/home/vmuser/RATP/RER/5_AUTRES/starter_web_app/node_modules/.pnpm/@adonisjs+core@6.12.1_@adonisjs+assembler@7.7.0_typescript@5.5.4__@vinejs+vine@2.1.0_edge.js@6.0.2/node_modules:/home/vmuser/RATP/RER/5_AUTRES/starter_web_app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/vmuser/RATP/RER/5_AUTRES/starter_web_app/node_modules/.pnpm/@adonisjs+core@6.12.1_@adonisjs+assembler@7.7.0_typescript@5.5.4__@vinejs+vine@2.1.0_edge.js@6.0.2/node_modules/@adonisjs/core/build/toolkit/node_modules:/home/vmuser/RATP/RER/5_AUTRES/starter_web_app/node_modules/.pnpm/@adonisjs+core@6.12.1_@adonisjs+assembler@7.7.0_typescript@5.5.4__@vinejs+vine@2.1.0_edge.js@6.0.2/node_modules/@adonisjs/core/build/node_modules:/home/vmuser/RATP/RER/5_AUTRES/starter_web_app/node_modules/.pnpm/@adonisjs+core@6.12.1_@adonisjs+assembler@7.7.0_typescript@5.5.4__@vinejs+vine@2.1.0_edge.js@6.0.2/node_modules/@adonisjs/core/node_modules:/home/vmuser/RATP/RER/5_AUTRES/starter_web_app/node_modules/.pnpm/@adonisjs+core@6.12.1_@adonisjs+assembler@7.7.0_typescript@5.5.4__@vinejs+vine@2.1.0_edge.js@6.0.2/node_modules/@adonisjs/node_modules:/home/vmuser/RATP/RER/5_AUTRES/starter_web_app/node_modules/.pnpm/@adonisjs+core@6.12.1_@adonisjs+assembler@7.7.0_typescript@5.5.4__@vinejs+vine@2.1.0_edge.js@6.0.2/node_modules:/home/vmuser/RATP/RER/5_AUTRES/starter_web_app/node_modules/.pnpm/node_modules:$NODE_PATH"
